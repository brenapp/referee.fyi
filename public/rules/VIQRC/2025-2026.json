            "name": "Scoring Rules",
            "programs": [
                "VIQRC"
            ],
            "rules": [
                {
                    "rule": "<SC1>",
                    "description": "All scoring statuses are evaluated after the Match ends, once all Scoring Objects, Field Elements, and Robots on the Field come to rest",
                    "link": "https://www.robotevents.com/storage/game_manual/VEX_IQ_Robotics_Competition_2025-2026_Mix_Match/rules/SC1.html"
                },
                {
                    "rule": "<SC2>",
                    "description": "All scoring statuses are evaluated visually by a Head Referee, to the best of their ability within the context of a given Match/event",
                    "link": "https://www.robotevents.com/storage/game_manual/VEX_IQ_Robotics_Competition_2025-2026_Mix_Match/rules/SC2.html"
                },
                {
                    "rule": "<SC3>",
                    "description": "A Scoring Object can be Connected to another Scoring Object to form a Stack",
                    "link": "https://www.robotevents.com/storage/game_manual/VEX_IQ_Robotics_Competition_2025-2026_Mix_Match/rules/SC3.html"
                },
                {
                    "rule": "<SC4>",
                    "description": "A Stack that includes more than one color (blue, red, orange, or gray) of Scoring Object receives additional points based on the number of colors in that Stack, up to three colors",
                    "link": "https://www.robotevents.com/storage/game_manual/VEX_IQ_Robotics_Competition_2025-2026_Mix_Match/rules/SC4.html"
                },
                {
                    "rule": "<SC5>",
                    "description": "A Stack is considered Placed in a Goal at the end of the Match if it meets all of the following criteria",
                    "link": "https://www.robotevents.com/storage/game_manual/VEX_IQ_Robotics_Competition_2025-2026_Mix_Match/rules/SC5.html"
                },
                {
                    "rule": "<SC6>",
                    "description": "A Stack earns a Matching Goal bonus when one or more of the following criteria is met:",
                    "link": "https://www.robotevents.com/storage/game_manual/VEX_IQ_Robotics_Competition_2025-2026_Mix_Match/rules/SC6.html"
                },
                {
                    "rule": "<SC7>",
                    "description": "A Starting Pin is Cleared if no part of its Starting Pin Support is within the volume of the Pin at the end of the Match",
                    "link": "https://www.robotevents.com/storage/game_manual/VEX_IQ_Robotics_Competition_2025-2026_Mix_Match/rules/SC7.html"
                },
                {
                    "rule": "<SC8>",
                    "description": "A Robot will receive 2 points for ending the Match in contact with Scoring Objects in the following scenarios:",
                    "link": "https://www.robotevents.com/storage/game_manual/VEX_IQ_Robotics_Competition_2025-2026_Mix_Match/rules/SC8.html"
                }
            ]
        },
