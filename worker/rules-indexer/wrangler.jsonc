{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "rules-indexer",
	"main": "src/index.ts",
	"compatibility_date": "2025-05-24",
	"observability": {
		"enabled": true
	},
	"triggers": {
		"crons": [
			"*/30 * * * *"
		]
	},
	"placement": {
		"mode": "smart"
	},
	"ai": {
		"binding": "AI"
	},
	"r2_buckets": [
		{
			"binding": "RULES_BUCKET",
			"bucket_name": "referee-fyi-game-rules",
		}
	]
}